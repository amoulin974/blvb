            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_journee_index') }}">back to list</a>

    <a href="{{ path('app_journee_edit', {'id': journee.id}) }}">edit</a>

    {{ include('journee/_delete_form.html.twig') }}
