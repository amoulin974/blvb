{% extends '../../base.html.twig' %}

{% block title %}Journee{% endblock %}

{% block body %}
    <h1>Journee</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ journee.id }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ journee.numero }}</td>
            </tr>
            <tr>
                <th>Date_debut</th>
